<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobiletermproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SchedulePopUp"
            android:theme="@style/Theme.AppCompat.Dialog"
            android:parentActivityName=".Calendar_main"/>
        <activity android:name=".DateSelection" android:theme="@style/Theme.AppCompat.Dialog"/>
        <activity android:name=".CreateSchedule" />
        <activity android:name=".JoinGroup"/>
        <activity android:name=".Loading" />
        <activity android:name=".Calendar_main" />
        <activity android:name=".LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActicity"
            android:label="@string/app_name"
            android:parentActivityName=".LoginActivity" />
    </application>

</manifest>